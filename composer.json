{
    "name":"villa-brocante/contao-google-shopping-feed",
    "type": "contao-bundle",
    "require": {
        "contao/core-bundle": "^4.10",
        "contao/news-bundle": "^4.10",
        "illuminate/collections": "^8.8",
        "sabre/xml": "^2.2",
        "vitalybaev/google-merchant-feed": "^2.0",
        "league/flysystem-bundle": "^1.5",
        "contao/manager-plugin": "^2.11"
    },
    "autoload": {
        "psr-4": {
            "VillaBrocante\\GoogleShoppingFeed\\": "src/"
        },
        "classmap": [
            "src/Resources/contao/"
        ],
        "exclude-from-classmap": [
            "src/Resources/contao/config/",
            "src/Resources/contao/dca/",
            "src/Resources/contao/languages/",
            "src/Resources/contao/templates/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "VillaBrocante\\GoogleShoppingFeed\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "contao/test-case": "^4.2",
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-latest"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "contao-manager-plugin": "VillaBrocante\\GoogleShoppingFeed\\ContaoManager\\Plugin"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    }
}
